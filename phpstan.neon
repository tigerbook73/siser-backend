includes:
  - ./vendor/nunomaduro/larastan/extension.neon

parameters:
  paths:
    - app
    - config
    # - database

  # The level 9 is the highest level
  level: 5

  ignoreErrors:
    # - "#PHPDoc tag @var#"

  excludePaths:
    - ./*/*/FileToBeExcluded.php

  checkMissingIterableValueType: false

  editorUrl: "vscode://file/%%file%%:%%line%%"
  editorUrlTitle: "%%relFile%%:%%line%%"
